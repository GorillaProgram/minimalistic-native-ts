        "react-native-animatable": "^1.1.0",
        "react-native-dropdownalert": "^2.4.0",
        "react-native-extended-stylesheet": "^0.3.1",
        "react-native-lazyload": "^1.1.0",
        "react-native-parallax": "^0.3.0",
        "react-native-scrollable-tab-view": "^0.7.1",
        "react-native-simple-router": "^0.12.0",
        "react-native-viewpager": "^0.2.13",
        "react-redux": "^5.0.2",
